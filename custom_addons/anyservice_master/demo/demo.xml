<odoo>
      <data noupdate="1">
        <record model="ir.module.category" id="module_category_anyservice_master_android_user">
                <field name="name">Android user</field>
                <field name="description">The Android user Applilcation</field>
                <field name="sequence">20</field>
        </record>
          <record id="android_user_group" model="res.groups">
              <field name="name">Android user</field>
              <field name="comment">Android user Permission Group.</field>
              <field name="category_id" ref="anyservice_master.module_category_anyservice_master_android_user"/>
          </record>
          <record model="ir.sequence" id="sequence_anyservice_order">
              <field name="name">Anyservice Order</field>
              <field name="code">anyservice.order</field>
              <field name="active">TRUE</field>
              <field name="prefix">ANY%(y)s%(month)s%(day)s</field>
              <field name="padding">6</field>
              <field name="number_next">1</field>
              <field name="number_increment">1</field>

          </record>
          <record model="ir.sequence" id="sequence_anyservice_product_code">
              <field name="name">Anyservice Product</field>
              <field name="code">anyservice.product</field>
              <field name="active">TRUE</field>
              <field name="prefix">P%(y)s%(month)s</field>
              <field name="padding">6</field>
              <field name="number_next">1</field>
              <field name="number_increment">1</field>

          </record>
    </data>
</odoo>
